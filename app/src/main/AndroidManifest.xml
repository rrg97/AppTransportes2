<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apps.android.rubeng.apptransportes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu_principal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Perfil"
            android:parentActivityName=".Menu_principal">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu_principal" />
        </activity>
        <activity
            android:name=".Mis_pedidos"
            android:parentActivityName=".Menu_principal">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu_principal" />
        </activity>
        <activity
            android:name=".Nuevo_pedido"
            android:parentActivityName=".Menu_principal">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu_principal" />
        </activity>
        <activity android:name=".Formulario" />
        <activity android:name=".Presupuesto" />
        <activity android:name=".Pago_pedido"></activity>
    </application>

</manifest>